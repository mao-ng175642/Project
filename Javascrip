
//Object//
var myBestFriend =
    {
      nickName: 'BanXau',
      age:20,
      gender :'male',
      isDog :true
    };
  myBestFriend['gender']='female';
console.log(myBestFriend);

var myGoodFriend =
     {
      nickName : 'QuyenCho',
      age:'20',
      yourUniversity :'HaNoiUniversityOfScienceAndTechnology',
      single :false,
      veryIntelligent : true
     };
   myGoodFriend['veryIntelligent']=false;
console.log(myGoodFriend);

var wetherToday =
     {
      haveSun : true,
      temperature : 26,
      haveWind : false
     };
console.log(wetherToday.temperature);
var all=[myBestFriend,myGoodFriend,wetherToday];
console.log(all);

